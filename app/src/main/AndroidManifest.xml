<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cogentdatasolutions.mydoctorapp">

    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission-sdk-23 android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/doclogo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DentistInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>
        <activity
            android:name=".DermaInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity
            android:name=".OrthoInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>
        <activity
            android:name=".PedInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>

        <activity
            android:name=".PhysicianInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>

        <activity
            android:name=".PsychoInfo"
            android:screenOrientation="portrait"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>

        </activity>

        <activity
            android:name=".information.DentDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".DentistInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DentistInfo"/>
        </activity>
        <activity
            android:name=".information.DermDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".DermaInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DermaInfo"/>
        </activity>
        <activity
            android:name=".information.OrthoDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".OrthoInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".OrthoInfo"/>
        </activity>
        <activity
            android:name=".information.PedDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".PedInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PedInfo"/>
        </activity>
        <activity
            android:name=".information.PhysicianDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".PhysicianInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PhysicianInfo"/>

        </activity>
        <activity
            android:name=".information.PsychoDoc"
            android:screenOrientation="portrait"
            android:parentActivityName=".PsychoInfo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PsychoInfo"/>
        </activity>
    </application>

</manifest>